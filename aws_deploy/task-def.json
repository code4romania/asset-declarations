{
    "family": "CatPol-test-task",
    "containerDefinitions": [
        {
            "name": "catpol",
            "image": "ovidiub13/catpol",
            "portMappings": [
                {
                    "hostPort": 8000,
                    "protocol": "tcp",
                    "containerPort": 8000
                },
                {
                    "hostPort": 80,
                    "containerPort": 80,
                    "protocol": "tcp"
                }
            ],
            "essential": true
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "256",
    "memory": "512",
    "networkMode": "awsvpc"
}
